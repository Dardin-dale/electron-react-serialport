{
  "name": "electron-serialport-react",
  "version": "1.0.0",
  "description": "A boilerplate for Electron for serialport use",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config ./webpack.config.js",
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "pack": "npm build && electron-builder --dir",
    "dist": "npm build && electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "build": {
    "appId": "serialplate"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "serial",
    "serialport"
  ],
  "author": "Logan Crecraft",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "electron": "^7.1.2",
    "electron-builder": "^22.7.0",
    "electron-rebuild": "^1.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "npm": "^6.14.5",
    "serialport": "^9.0.0"
  }
}
